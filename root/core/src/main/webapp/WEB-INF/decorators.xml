<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/api/*</pattern>
		<pattern>/help.jsp</pattern>
		<pattern>/icon.jsp</pattern>
	</excludes>
	
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
	<decorator name="http-service" page="http-service.jsp">
	</decorator>
	<decorator name="none">
	</decorator>
</decorators>
